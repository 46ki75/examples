{
  "name": "Next.js Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "/bin/bash .devcontainer/setup.sh",
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "usernamehw.errorlens",
        "Gruntfuggly.todo-tree",
